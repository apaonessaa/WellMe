<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <solid android:color="@color/md_theme_inverseSurface_mediumContrast"/>
            <stroke android:width="2dp" android:color="@color/md_theme_outline"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <solid android:color="@color/md_theme_background_highContrast"/>
            <stroke android:width="2dp" android:color="@color/md_theme_primaryContainer_mediumContrast"/>
        </shape>
    </item>
</layer-list>
